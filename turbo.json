{
  "$schema": "https://json.schemastore.org/turbo",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false
    },
    "prisma:migrate": {
      "cache": false
    },
    "prisma:seed": {
      "cache": false
    }
  }
}